        <definition>void uart_config_init</definition>
        <argsstring>(chanend c_uart_config, NULLABLE_RESOURCE(chanend, c_flash_data), NULLABLE_RESOURCE(chanend, c_xtcp), REFERENCE_PARAM(int, telnet_port_address))</argsstring>
        <name>uart_config_init</name>
        <param>
          <type>chanend</type>
          <declname>c_uart_config</declname>
        </param>
        <param>
          <type>NULLABLE_RESOURCE(chanend, c_flash_data)</type>
